{
  "name": "@alexlit/vue-yandex-share",
  "main": "./dist/vue-yandex-share.common.js",
  "private": false,
  "version": "1.0.1",
  "description": "Vue Yandex Share component",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alex-lit/vue-yandex-share.git"
  },
  "keywords": [
    "vue",
    "yandex",
    "social buttons",
    "social share",
    "social",
    "share",
    "buttons",
    "component"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alex-lit/vue-yandex-share/issues"
  },
  "author": "Alexey Litovchenko",
  "maintainers": [
    {
      "name": "Alexey Litovchenko",
      "email": "alex.lit@outlook.com",
      "url": "https://alexlit.gitlab.io"
    }
  ],
  "homepage": "https://github.com/alex-lit/vue-yandex-share#readme",
  "scripts": {
    "serve": "vue-cli-service serve & chokidar './src/**/*.vue' -c \"if [ '{event}' = 'change' ]; then stylelint {path} --fix; fi;\"",
    "build": "cross-env OUTPUT_DIR=docs vue-cli-service build & npm run build:lib",
    "build:lib": "vue-cli-service build --target lib --name vue-yandex-share ./src/components/vue-yandex-share/vue-yandex-share.vue",
    "lint": "vue-cli-service lint",
    "eslint": "eslint --ext .js,.ts,.vue ./src --fix",
    "stylelint": "stylelint 'src/**/*.css' 'src/**/*.pcss' 'src/**/*.vue' --fix",
    "up": "npx npm-check-updates -u & npm i && npm audit fix"
  },
  "files": [
    "src/components/vue-yandex-share/*",
    "dist/*"
  ],
  "dependencies": {
    "@alexlit/css-material-color-palette-variables": "^1.0.5",
    "bowser": "^2.8.1",
    "css-has-pseudo": "^1.0.0",
    "element-ui": "^2.13.0",
    "normalize.css": "^8.0.1",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.2",
    "vue-property-decorator": "^8.3.0",
    "vue2-github-corners": "^0.1.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "@vue/cli-plugin-eslint": "^4.1.2",
    "@vue/cli-plugin-typescript": "^4.1.2",
    "@vue/cli-service": "^4.1.2",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "@vue/eslint-config-typescript": "^5.0.1",
    "chokidar-cli": "^2.1.0",
    "commitizen": "^4.0.3",
    "cq-prolyfill": "^0.4.0",
    "cross-env": "^6.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-jsdoc": "^20.3.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-simple-import-sort": "^5.0.0",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-vue": "^6.1.2",
    "husky": "^4.0.10",
    "lint-staged": "^10.0.1",
    "postcss-color-mod-function": "^3.0.3",
    "postcss-each": "^0.10.0",
    "postcss-for": "^2.1.1",
    "postcss-html": "^0.36.0",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.2.1",
    "postcss-object-fit-images": "^1.1.2",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.19.1",
    "prettier-stylelint": "^0.4.2",
    "stylelint": "^13.0.0",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-standard": "^19.0.0",
    "typescript": "~3.7.5",
    "vue-template-compiler": "^2.6.11"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --write",
      "npm run eslint",
      "git add"
    ],
    "src/**/*.ts": [
      "prettier --write",
      "npm run eslint",
      "git add"
    ],
    "src/**/*.vue": [
      "prettier --write",
      "npm run eslint",
      "npm run stylelint",
      "git add"
    ],
    "src/**/*.css": [
      "npm run stylelint",
      "git add"
    ],
    "src/**/*.pcss": [
      "npm run stylelint",
      "git add"
    ],
    "*.md": [
      "prettier --write",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  }
}
